<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".LauncherActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/hello_world"
        android:id="@+id/console"
        android:layout_below="@+id/feeds"
        android:layout_alignParentLeft="true"/>

    <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Get Feeds"
            android:id="@+id/feeds"
            android:onClick="getFeeds"
            android:layout_alignParentTop="true"
            android:layout_alignLeft="@+id/console"/>

    <EditText
            android:layout_width="160dp"
            android:layout_height="wrap_content"
            android:id="@+id/queryBox"
            android:layout_alignBottom="@+id/feeds"
            android:layout_toRightOf="@+id/feeds"
            android:layout_marginLeft="51dp"/>

</RelativeLayout>
